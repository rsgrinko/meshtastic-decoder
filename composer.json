{
    "name": "rsgrinko/meshtastic-decoder",
    "type": "library",
    "description": "PHP Library to work with Meshtastic protobuf encoded and encrypted data",
    "keywords": [
        "rsgrinko",
        "meshtastic",
        "protobuf",
        "proto",
        "encryption",
        "serviceenvelope",
        "meshpacket"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Grinko",
            "email": "rsgrinko@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "rsgrinko/meshtastic-proto": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rsgrinko\\MeshtasticDecode\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
